    "underscore": "*",
    "spine": "~1.0.8",
    "ubret" : "git+ssh://git@github.com:zooniverse/Ubret.git#HEAD",
    "zooniverse" : "git+ssh://git@github.com:zooniverse/Zooniverse.git#HEAD",
    "node-pubsub" : "*"
  },
  "devDependencies" : {
    "jasmine-phantom-node" :  "git://github.com/edpaget/jasmine-phantom-node.git",
    "sinon"                :  "*",
