<h4>Tool Settings</h4>
<% if @keys: %>
  <select name="key" class="select-key" id="select-key">
    <% for key in @keys: %>
      <% if key is @currentKey: %>
        <option name="key" value="<%- key %>" selected><%- key %></option>
      <% else: %>
        <option name="key" value="<%- key %>"><%- key %></option>
      <% end %>
    <% end %>
  </select>
<% else: %>
  No data.
<% end %>