<h4>Data Source</h4>
<div class="internal">
  <select class="sources">
    <option value="">Select Tool</option>
    <% for source in @intSources: %>
      <% if source.id is @source: %>
        <option value="<%- source.id%>" selected><%- source.name %></option>
      <% else: %>
        <option value="<%- source.id%>"><%- source.name %></option>
      <% end %>
    <% end %>
  </select>
</div>
